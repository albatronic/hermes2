{# edit.js #}

{% extends '_global/js.html.twig' %}

{% block js %}

    {{ parent() }}

    <script type="text/javascript">
        $("#docs").click(function () {
            url = "<iframe src='{{app.path}}/CpanDocs/listPopup/{{values.controller}}/{{datos.PrimaryKeyValue}}/document/1' width='100%' height='270' scrolling='yes' style='border: 0px dotted grey;'></iframe>";
            $("#divDocs").html(url);
        });
        $("#detalle").click(function () {
            url = "<iframe src='{{app.path}}/AlbaranesLineas/list/{{datos.IDAlbaran}}' width='100%' height='400' margin='0' scrolling='yes' style='border: 0px dotted grey;'></iframe>";
            $("#divDetalle").html(url);
        });
    </script>      
{% endblock %}
