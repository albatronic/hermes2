                {{ macro.input(values.atributos.RecargoEnergetico,'EtiquetaLarga','checkbox','Articulos[RecargoEnergetico]','Articulos_RecargoEnergetico',datos.RecargoEnergetico.IDTipo,'255','CampoTextoLargo','20')}}                          
            </div>
            <div class="col-md-4">           
                {{ macro.input(values.atributos.PackingCompras,'EtiquetaLarga','text',values.controller ~ '[PackingCompras]',values.controller ~ '_PackingCompras',datos.PackingCompras,'10','CampoImporte','24') }}
