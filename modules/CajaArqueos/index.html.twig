{# Template index.html.twig for CajaArqueos #}
{# ----------------------------------------------------#}
{% extends layout %}

{% block title parent() ~ ' - '  ~ values.titulo %}

{% block content %}
	<div class='Titulo'>{{ values.titulo }}
	{% block navegador %}
            {% include '_global/comandosNew.html.twig' with {'controller':values.controller} %}
	{% endblock %}
        {% include '_global/comandosHelp.html.twig' with {'controller':values.controller} %}
        </div>

	{% block filtro %}
        {% if values.permisos.permisosModulo['CO'] %}
            {% include 'CajaArqueos/filtro.html.twig' with {'filter':values.listado.filter, 'alertas':values.alertas} %}
        {%endif%}
	{% endblock %}

        {% block mantenimiento %}
        {% endblock %}

        <div id="div_listado">
        {% block listado %}
	{% endblock %}
        </div>
{% endblock %}