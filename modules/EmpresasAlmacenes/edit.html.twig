{% extends 'EmpresasAlmacenes/index.html.twig' %}

{% block navegador %}
{% include "_global/comandosList.html.twig" with {'controller':'EmpresasAlmacenes', 'linkValue': values.linkBy.value} %}
{% include "_global/comandosNew.html.twig" with {'controller':'EmpresasAlmacenes', 'linkValue': values.linkBy.value} %}
{% endblock %}

{% block mantenimiento %}
<div class="FormManto">
    <form name="manto_{{values.controller}}" id="manto_{{values.controller}}" action="" method="POST" enctype="multipart/form-data">
        <input name="controller" value="EmpresasAlmacenes" type="hidden" />
        <input name="action" value="edit" type="hidden" />
        <input name="EmpresasAlmacenes[IDEmpresa]" value="{{ values.linkBy.value }}" type="hidden" />
        <input name="EmpresasAlmacenes[Id]" value="{{ values.datos.ID }}" type="hidden" />
        {% include "_global/comandosSaveDelete.html.twig" %}

        <div class='Cuerpo'>
	{% include "_global/FormErrores.html.twig" with {'errores': values.errores} %}
	{% include "EmpresasAlmacenes/form.html.twig" with {'datos': values.datos} %}
        </div>
    </form>
</div>
{% endblock %}