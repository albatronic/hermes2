{# js Propiedades #}

{% extends '_global/js.html.twig' %}

{% block js %}

    {{ parent() }}
    <script type="text/javascript">
        function limpia(idOrigen, idInput) {
            if ($('#' + idOrigen + '_valorAnterior').val() !== $('#' + idOrigen).val()) {
                $('#' + idInput).val('');
                $('#' + idInput + '_autoComplete').val('');
            }
        }

        function cargaValores(idDiv,idLinkBy) {
            $('#' + idDiv).html("<iframe width='100%' height='330' scrolling='yes' style='margin-top:5px; border:0;' src='{{app.path}}/PropiedadesValores/list/"+idLinkBy+"'></iframe>");
        }
    </script>
{% endblock %}
